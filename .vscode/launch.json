    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start Osmosis Node - Debug",
            "request": "launch",
            "type": "go",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/osmosisd/main.go",
            "args": [
                "start",
                "--home",
                "${workspaceFolder}/../osmosis-price-indexer",
                "--halt-height",
                "7200000",
                //"--log_level",
                "--trace"
                //"debug"
            ],
        },
        {
            "name": "Init Osmosis Node ",
            "request": "launch",
            "type": "go",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/osmosisd/main.go",
            "args": [
                "init",
                "Splather",
                "--home",
                "/Users/richard/workspace/osmosis-price-indexer",
                //"--log_level",
                //"debug"
            ],
        },
    ]
}